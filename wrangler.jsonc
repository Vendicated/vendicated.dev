{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "vendicated-dev",
    "main": "./dist/_worker.js/index.js",
    // Update to today's date
    "compatibility_date": "2025-09-20",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "binding": "ASSETS",
        "directory": "./dist"
    },
    "observability": {
        "enabled": true
    }
}